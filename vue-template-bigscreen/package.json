{
    "name": "vue-ts-template",
    "version": "1.0.0",
    "private": false,
    "description": "基于 vue-cli3 + TypeScript 搭建的多项目前端模板",
    "bin": {},
    "scripts": {
        "api": "node api-server",
        "lint": "stylelint **/*.{css,less,html,vue} --fix && vue-cli-service lint",
        "build:test:zql-admin": "cross-env VUE_APP_ENTRY=zql-admin vue-cli-service build --mode=test",
        "changelog": "sh build/git-log.sh -m changelog -f",
        "dev:vue-template-bigscreen": "cross-env VUE_APP_ENTRY=vue-template-bigscreen vue-cli-service serve --open",
        "dev:vue-template-mobile": "cross-env VUE_APP_ENTRY=vue-template-mobile vue-cli-service serve --open",
        "dev:vue-template-admin": "cross-env VUE_APP_ENTRY=vue-template-admin vue-cli-service serve --open",
        "dev:vue-template-homesite": "cross-env VUE_APP_ENTRY=vue-template-homesite vue-cli-service serve --open",
        "dev:component": "cross-env VUE_APP_ENTRY=component vue-cli-service serve --open",
        "dist": "node ./static-server.js",
        "new": "plop",
        "zip": "node build/zip",
        "new:mock": "plop mock",
        "report": "npm run build -- --report",
        "tag": "sh build/git-log.sh -m publish -f",
        "test": "vue-cli-service test:unit",
        "deploy": "echo 'todo'",
        "docs": "typedoc --out ./doc/ ./src && npm run serve:docs",
        "serve:docs": "http-server ./doc -p 3001 -o",
        "build:vue-template-admin": "cross-env VUE_APP_ENTRY=vue-template-admin vue-cli-service build --mode=production",
        "build:vue-template-homesite": "cross-env VUE_APP_ENTRY=vue-template-homesite vue-cli-service build --mode=production",
        "dev:integrates-bigscreen": "cross-env VUE_APP_ENTRY=integrates-bigscreen vue-cli-service serve --open",
        "build:test:integrates-bigscreen": "cross-env VUE_APP_ENTRY=integrates-bigscreen vue-cli-service build --mode=test",
        "build:fat:integrates-bigscreen": "cross-env VUE_APP_ENTRY=integrates-bigscreen vue-cli-service build --mode=fat",
        "build:pre:integrates-bigscreen": "cross-env VUE_APP_ENTRY=integrates-bigscreen vue-cli-service build --mode=pre",
        "build:prd:integrates-bigscreen": "cross-env VUE_APP_ENTRY=integrates-bigscreen vue-cli-service build --mode=production"
    },
    "dependencies": {
        "@jiaminghi/data-view": "^2.10.0",
        "@riophae/vue-treeselect": "^0.4.0",
        "axios": "~0.19.2",
        "babel-polyfill": "~6.26.0",
        "clipboard": "~2.0.8",
        "core-js": "~3.6.5",
        "date-fns": "~1.30.1",
        "downloadjs": "~1.4.7",
        "echarts": "^4.8.0",
        "echarts5": "npm:echarts@^5.3.2",
        "element-ui": "~2.13.2",
        "highlight.js": "~10.1.1",
        "hx-zj-map": "git+ssh://git@gitlab.icinfo.co:fed/icinfo.map-base.git#v2.0.0",
        "jsrsasign": "~10.1.5",
        "lodash": "~4.17.20",
        "mockjs": "~1.1.0",
        "normalize.css": "~8.0.1",
        "nprogress": "~0.2.0",
        "print-js": "~1.5.0",
        "qs": "~6.9.4",
        "quill": "~1.3.7",
        "sharegood-cloud-utils": "0.0.7",
        "svgaplayerweb": "^2.3.2",
        "url-parse": "~1.4.7",
        "vant": "~2.10.2",
        "vconsole": "~3.3.4",
        "vue": "~2.6.12",
        "vue-class-component": "~7.2.3",
        "vue-property-decorator": "~9.1.2",
        "vue-quill-editor": "~3.0.6",
        "vue-router": "~3.4.3",
        "vue-seamless-scroll": "^1.1.23",
        "vuex": "~3.6.2",
        "vuex-class": "~0.3.2",
        "vuex-persistedstate": "~4.0.0-beta.1",
        "xlsx": "~0.16.8",
        "xss": "~1.0.8"
    },
    "devDependencies": {
        "@babel/cli": "~7.11.6",
        "@babel/core": "~7.11.6",
        "@babel/node": "~7.10.5",
        "@babel/preset-env": "~7.11.5",
        "@types/jest": "~24.0.19",
        "@types/lodash": "~4.14.159",
        "@types/nprogress": "~0.2.0",
        "@typescript-eslint/eslint-plugin": "~4.18.0",
        "@typescript-eslint/parser": "~4.18.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-typescript": "~4.5.12",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/eslint-config-prettier": "~6.0.0",
        "@vue/eslint-config-typescript": "~7.0.0",
        "@vue/test-utils": "~1.0.4",
        "api-server": "git+ssh://git@gitlab.icinfo.co:fed/tool.api.git#v0.2.4",
        "archiver": "~5.3.0",
        "babel-eslint": "~10.1.0",
        "babel-plugin-component": "~1.1.1",
        "babel-plugin-lodash": "~3.3.4",
        "compression-webpack-plugin": "~6.0.2",
        "cross-env": "~7.0.3",
        "eslint": "~6.8.0",
        "eslint-plugin-html": "~6.0.3",
        "eslint-plugin-prettier": "~3.1.4",
        "eslint-plugin-vue": "~6.2.2",
        "express": "~4.17.1",
        "http-proxy-middleware": "~1.0.4",
        "http-server": "~0.12.3",
        "less": "~3.11.3",
        "less-loader": "~5.0.0",
        "lint-staged": "~9.5.0",
        "open": "~7.2.0",
        "plop": "~2.7.4",
        "postcss-pxtorem": "~5.1.1",
        "prettier": "~1.19.1",
        "raw-loader": "~4.0.1",
        "stylelint": "~13.6.1",
        "stylelint-config-prettier": "~8.0.2",
        "stylelint-order": "~4.1.0",
        "svg-sprite-loader": "~4.3.0",
        "svgo": "~1.3.2",
        "svgo-loader": "~2.2.1",
        "ts-node": "~9.1.1",
        "typedoc": "~0.20.34",
        "typescript": "~4.1.5",
        "validate-commit-msg": "~2.14.0",
        "vue-count-to": "^1.0.13",
        "vue-template-compiler": "~2.6.12"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "since 2016",
        "edge >= 12",
        "not ie <= 10",
        "not dead",
        "Android >= 4",
        "iOS >= 8"
    ],
    "jest": {
        "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
    },
    "gitHooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "validate-commit-msg"
    },
    "lint-staged": {
        "src/*.{css,less,html,vue}": [
            "stylelint --aei --fix",
            "git add"
        ],
        "src/*.{js,jsx,mjs,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    }
}
